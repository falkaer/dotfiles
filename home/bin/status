# will fail if all strings are empty, but cba
join(){
    
    if [ $# -eq 0 ]; then
        return
    fi
    
    msg=""
    shift
    
    while [ $# -ge 1 ]; do
        [ ! -z "$1" ] && msg+="$1\n"
        shift
    done
    
    echo "${msg::-2}"
    
}

msg="$(join "$(~/bin/pow)" "$(~/bin/cha)" "$(~/bin/net)")"
notify-send "$(~/bin/dat)" "$msg"