# will fail if all strings are empty, but cba
join(){
    
    if [ $# -eq 0 ]; then
        return
    fi
    
    msg=""
    
    while [ $# -ge 1 ]; do
        [ ! -z "$1" ] && msg+="$1\n"
        shift
    done
    
    echo "${msg::-2}"
    
}

msg="$(join "$(~/.scripts/cal)" "$(~/.scripts/pow)" "$(~/.scripts/net)")"
notify-send "$(~/.scripts/dat)" "$msg"