#!/usr/bin/env python3

import os
import os.path as osp

def find_orphans(curdir):
    orphans = []
    for f in os.listdir(curdir):
        if osp.isdir(f):
            if len(find_orphans(f)) == 0:
                orphans.append(f)
        elif osp.isfile(f):
            if os.stat(f).st_nlink > 1:
                orphans.append(f)
    return orphans

if __name__ == '__main__':
    orphans = find_orphans(os.getcwd())
    for o in orphans:
        print(o)